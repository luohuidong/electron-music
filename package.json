{
  "name": "snow-music",
  "version": "1.2.0",
  "description": "An electron music app",
  "private": true,
  "main": "dist-webpack-main/index.js",
  "scripts": {
    "renderer:dev": "cross-env NODE_ENV=development webpack serve --hot --config webpack/webpack.renderer.dev.js",
    "renderer:build": "corss-env NODE_ENV=production webpack --config webpack/webpack.renderer.prod.js",
    "renderer:analyze-bundle": "ts-node script/analyzeBundle.ts",
    "main:dev": "webpack --config webpack/webpack.main.dev.js && cross-env NODE_ENV=development electron .",
    "main:build": "webpack --config webpack/webpack.main.prod.js",
    "main:debug": "rm -rf dist-webpack-main && rm -rf tmp && tsc --project tsconfig.maindebug.json",
    "build": "npm run renderer:build && npm run main:build",
    "electron-pack": "npm run build && electron-builder --config electron-builder.json",
    "electron-pack-dir": "npm run build && electron-builder --dir --config electron-builder.json",
    "electron-pack-all": "npm run build && electron-builder -mwl --config electron-builder.json",
    "publish": "npm run build && electron-builder -mwl --config electron-builder.json -p always",
    "commit": "cz",
    "prettier": "prettier --config-precedence prefer-file --write .",
    "check": "tsc"
  },
  "keywords": [],
  "author": "luohuidong",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/luohuidong/electron-music.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/classnames": "^2.3.1",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/mini-css-extract-plugin": "^1.4.3",
    "@types/node": "^14.17.14",
    "@types/react": "^16.14.14",
    "@types/react-dom": "^16.9.14",
    "@types/react-redux": "^7.1.18",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^10.4.7",
    "electron-builder": "^22.11.7",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.25.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^1.6.2",
    "nodemon": "^2.0.12",
    "postcss": "^8.3.6",
    "postcss-loader": "^4.3.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.2",
    "sass": "^1.38.2",
    "sass-loader": "^10.2.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.2.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.4.2",
    "webpack": "^5.51.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^17.0.1",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "core-js": "^3.16.4",
    "electron-log": "^4.4.1",
    "electron-updater": "^4.3.9",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.1",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
